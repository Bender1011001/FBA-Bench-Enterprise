    {
      "type": "timeseries",
      "title": "Latency p95 (seconds)",
      "description": "Latency derived from OpenTelemetry spanmetrics histogram",
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green" }, { "color": "orange", "value": 2 }, { "color": "red", "value": 3 } ] }
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(traces_spanmetrics_latency_bucket[5m])))",
          "legendFormat": "p95 latency"
        }
      ],
      "gridPos": { "x": 0, "y": 1, "w": 12, "h": 8 },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "single", "sort": "none" }
      }
    },
    {
      "type": "timeseries",
      "title": "Error Rate (%)",
      "description": "5xx error percentage from spanmetrics",
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green" }, { "color": "orange", "value": 5 }, { "color": "red", "value": 10 } ] }
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "( sum(rate(traces_spanmetrics_calls_total{http_status_code=~\"5..\"}[5m])) ) / ( sum(rate(traces_spanmetrics_calls_total[5m])) ) * 100",
          "legendFormat": "5xx %"
        }
      ],
      "gridPos": { "x": 12, "y": 1, "w": 12, "h": 8 },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "single", "sort": "none" }
      }
    },
    {
      "type": "timeseries",
      "title": "Throughput (RPS)",
      "description": "Requests per second from spanmetrics",
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "fieldConfig": {
        "defaults": { "unit": "req/s" },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(traces_spanmetrics_calls_total[1m]))",
          "legendFormat": "rps"
        }
      ],
      "gridPos": { "x": 0, "y": 9, "w": 12, "h": 7 },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "single", "sort": "none" }
      }
    },
